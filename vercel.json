{
  "buildCommand": "cd frontend-main && npm install && npm run build",
  "outputDirectory": "frontend-main/dist",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend-main/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
